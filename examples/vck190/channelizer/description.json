{
    "name": "Channelizer Kernel", 
    "flow": "system",
    "description": ["Channelizer kernel."],
    "keywords": [],
    "key_concepts": [],
    "os": ["linux", "aie_runtime"],
    "platform_allowlist": [
        "xilinx_vck190_base_202520_1"
    ],
    "gui": true,
    "vitis_flow" : "sw-test-harness",
    "input_value" : "vck190_test_harness.xsa",
    "v++": {
      "package": {
        "cflags": [
            "--package.defer_aie_run",
            "--advanced.param package.enableAiePartitionDrc=0"
        ]
      }
    },
    "host": {
      "compiler": {
        "sources": [
          "ps/host.cpp"
        ],
        "includepaths": [
          "REPO_DIR/include/ps"
        ],
        "options": "-std=c++17"
      }
    },
    "aiecompiler": {
      "compiler": {
        "includepaths": [
          "./aie",
          "REPO_DIR/include/aie"
        ]
      }
    },
    "aiecontainers": [
      {
        "accelerators": [
          {
            "location": "aie/m16_ssr8_app.cpp",
            "option": "-log-level=1 --stacksize=1024 --heapsize=1024 --pl-freq=312.5 --event-trace=runtime --event-trace-port=gmio",
            "name": "libadf.a",
              "aie_kernel_files": [
                  "aie/polyphase_fir.cpp",
                  "aie/dft_1xN_widget.cpp"
              ]
          }
        ]
      }
    ],
    "launch": [],
    "contributors": [
      {
        "url": "http://www.xilinx.com",
        "group": "Xilinx"
      }
    ],
    "testinfo": {
        "disable": false,
        "retry":3,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
